{
  "name": "@cvai/client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "apollo-upload-client": "^18.0.1",
    "formik": "^2.4.6",
    "graphql": "^16.9.0",
    "jiti": "^2.4.0",
    "jsondiffpatch": "^0.6.0",
    "openapi-fetch": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "latest",
    "react-dropzone": "^14.3.6",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^10.0.6",
    "ts-pattern": "^5.5.0",
    "tslib": "^2.6.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "gh-pages:deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "graphql-codegen": "graphql-codegen --config codegen.ts",
    "openapi-codegen": "openapi-typescript http://localhost:4000/api-docs-json --output src/generated/api-types.ts",
    "lint:check": "eslint src/**/*.{ts,tsx} --ignore-pattern '**/generated/**/*' --no-warn-ignored",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "prettier --check ./src/**/*.{ts,tsx,js,jsx,cjs}",
    "prettier:fix": "prettier --write ./src/**/*.{ts,tsx,js,jsx,cjs}",
    "tsc:check": "tsc --noEmit",
    "codecheck": "yarn tsc:check && yarn lint:check && yarn prettier:check",
    "codecheck:fix": "yarn lint:fix && yarn prettier:fix"
  },
  "devDependencies": {
    "@cvai/tools": "*",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@parcel/watcher": "^2.4.1",
    "@types/apollo-upload-client": "^18",
    "@types/gh-pages": "^6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.36.1",
    "gh-pages": "^6.3.0",
    "openapi-typescript": "^7.4.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
