{
  "name": "@c-v-a-i/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch",
    "start:prod:web-server": "node --max_old_space_size=1024 dist/apps/web-server/apps/web-server/src/main",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/web-server/src/test/jest-e2e.json",
    "typeorm": "node --require ts-node/register -r tsconfig-paths/register ../../node_modules/typeorm/cli.js -d apps/web-server/data-source.ts",
    "migration:generate": "sh -c 'yarn typeorm migration:generate ./apps/web-server/src/migrations/$0'",
    "migration:create": "sh -c 'typeorm migration:create ./apps/web-server/src/migrations/$0'",
    "migration:revert": "yarn typeorm migration:revert",
    "migration:run": "yarn typeorm migration:run",
    "test": "jest --passWithNoTests",
    "lint": "eslint {apps,libs,config}/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "prettier --check {apps,libs,config}/**/*.{ts,tsx}",
    "prettier:fix": "prettier --write {apps,libs,config}/**/*.{ts,tsx}",
    "codecheck": "tsc --noEmit && yarn lint && yarn prettier:check",
    "codecheck:fix": "yarn lint:fix && yarn prettier:fix",
    "db:create": "chmod +x ./scripts/db_create.sh && ./scripts/db_create.sh",
    "db:restore": "chmod +x ./scripts/db_restore.sh && ./scripts/db_restore.sh",
    "db:import": "yarn db:create && yarn db:restore"
  },
  "peerDependencies": {
    "@types/node": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "jest": "*",
    "prettier": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@langchain/community": "^0.3.20",
    "@langchain/core": "^0.3.26",
    "@langchain/openai": "^0.3.16",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/testing": "^10.4.8",
    "@nestjs/typeorm": "^10.0.2",
    "@types/lodash": "^4.17.13",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dataloader": "^2.2.2",
    "graphql": "^16.9.0",
    "graphql-upload-ts": "^2.1.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.8.4",
    "openai": "^4.85.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@c-v-a-i/tools": "*",
    "@types/cookie-parser": "^1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4",
    "@types/pg": "^8",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "pg": "^8.13.1",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3"
  },
  "resolutions": {
    "@langchain/core": "0.3.7"
  }
}
